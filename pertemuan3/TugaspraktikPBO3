public class Main {
    public static void main(String[] args) {
        Mobil mobil1 = new Mobil("BMW", "X5", 2021, "Putih");
        Mobil mobil2 = new Mobil("Tesla", "Model S", 2022, "Hitam");
        
        mobil1.displayinfo();
        mobil1.displayinfo();
        
        mobil1.startEngine();
        mobil2.startEngine();
        
        mobil1.ubahWarna("Abu-abu");
        mobil1.displayinfo();
    }
}
